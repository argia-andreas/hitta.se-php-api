{
    "name": "grafstorm/hitta_php_package",
    "description": "Hitta.se PHP package",
    "keywords": [
        "grafstorm",
        "hitta_php_package"
    ],
    "homepage": "https://github.com/grafstorm/hitta_php_package",
    "license": "MIT",
    "authors": [
        {
            "name": "grafi",
            "email": "andreas@grafstrom.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "spatie/enum": "^3.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Grafstorm\\Hitta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grafstorm\\Hitta\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
